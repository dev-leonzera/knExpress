    "url": "https://github.com/dev-leonzera/knExpress.git/issues"
  },
  "homepage": "https://github.com/dev-leonzera/knExpress.git#readme",
  "dependencies": {
    "bcrypt": "5.0.0",
    "celebrate": "12.1.2",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "knex": "2.4.0",
    "morgan": "1.10.0",
    "mysql2": "2.1.0",
    "objection": "2.2.1",
